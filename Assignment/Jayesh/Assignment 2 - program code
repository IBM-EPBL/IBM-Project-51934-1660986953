import random
import time
while True:
    try:
        time.sleep(3)
        temp = random.randrange(20, 50)
        humid = random.randrange(10, 100)
        if temp >= 40:
            print(temp, "The temperature is high!!- ALARM")
        else:
            print(temp, "The temperature is normal")
        if humid >= 70:
            print(humid, "The humidity is high!!- ALARM")
        else:
            print(humid, "The humidity is normal")
    except KeyboardInterrupt:
        break
